        $(".my-src-input").val('');
    });

    var kw_url = "/buy?r=l&kw=%E5%A5%B3%E8%A3%85%E5%A4%8F";

    $('#search button').on('click',function(){
        var _this = $(this);
        if($(this).siblings('input[name="kw"]').val()==''&& kw_url != ""){
            window.location.href=$("#m_url").val();
            return false;
        }else{
            _this.parents('form').attr('action',$("#h_url").val())
        }
    })

    $('.floatNav .srcBtn').on('click',function(){
        var _this = $(this);
        if($('input[name="kw"]').val()==''&& kw_url != "" ){
            window.location.href=$("#m_url").val();
            return false;
        }else{
            _this.parents('form').attr('action',$("#h_url").val())
        }
    });

</script>

<script type="text/javascript" src="http://cmsstatic.dataoke.com//js/scrollTop2.js?v=201805101736"></script>
</body>
</html>
";}